local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local snipers = { "FanchonVolcano", "AstroPilot_X", "yunidmailof", "numifaisi" }
local plaza = { "FanchonRiver" }

-- Ensure that LocalPlayer is available
repeat task.wait() until LocalPlayer and LocalPlayer.Name

-- Check if the player's account name matches any in the list and execute the corresponding script
if table.find(snipers, LocalPlayer.Name) then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ReimuSimpu/Lua-Shit/refs/heads/main/Relic.lua", true))()
elseif table.find(plaza, LocalPlayer.Name) then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ReimuSimpu/Lua-Shit/refs/heads/main/Auto%20Mail.lua", true))()
end
